<h1>Add a New Member</h1>

<form>
  
	<label>First Name: 
  <input
    type="text"
    name="fname"
    value="<%= member.escape("fname") %>">
  </label>
  <br>

  <label>Last Name: 
  <input
    type="text"
    name="lname"
    value="<%= member.escape("lname") %>">
  </label>
  <br>

  <label>Role (optional): 
  <input
    type="text"
    name="role"
    value="<%= member.escape("role") %>">
  </label>
  <br>

  <label>Short Bio (optional): 
  <input
    type="textarea"
    name="bio"
    value="<%= member.escape("bio") %>">
  </label>
  <br>
  <br>

  <input type="submit" value="&#10003">
  <button class="cancel">X</button>
  
</form>
