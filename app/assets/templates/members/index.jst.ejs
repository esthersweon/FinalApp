<div class="container">

	<div class="row">

	<div class= "col-content">
	<img src="/images/pyramid.jpg" alt="Pyramid" width="200" height="300">
	</div>

	<div class= "col-content">
	<h4>||   at a glance   ||</h4>

<ul>
<li><strong># members</strong> - <%= members.length %></li>
	<p></p>
	<% members.each (function(member) { %>
		<li class="member">
			<a href="#/members/<%= member.id %>"><%= member.escape("fname") + " " + member.escape("lname") %></a> - <p><%= member.escape("role") %></p>
		</li>
	<% }) %>
	<br>
	<button class="newMember">create a member</button>
</ul>
</div>

	<div class= "col-longest">
		<a href="#" id="back"><h4>||   back to dashboard   ||</h4></a>
		<br>
		<div id="members-new"></div>
	</div>


</div>
</div>