<form role="form">

  <div class="form-group">
    <input type="text"
    id="phase_title"
    name="phase[title]"
    value="<%= phase.escape("title") %>"
    placeholder="phase title"
    class="form-control">
  </div>

  <div class="form-group">
    <input type="date"
    id="phase_end_date"
    name="phase[end_date]"
    value="<%= phase.escape("end_date") %>"
    placeholder="end date"
    class="form-control">
  </div>

  <input type="submit">
</form>